          xmlns:tools="http://schemas.android.com/tools"
          split="feature1"
          package="com.ketkigrag.androidappbundles.ondemand">

    <dist:module
            dist:onDemand="true"
            dist:title="feature1"
            tools:node="strict">
        <dist:fusing dist:include="true" />
    </dist:module>



    <application>
        <activity android:name="com.ketkigrag.androidappbundles.ondemand.LaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
    </application>

</manifest>